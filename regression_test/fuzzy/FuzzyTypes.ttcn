// This module contains types for testing fuzzy parameters
module FuzzyTypes {

type port PT message {
  inout integer
}
with {
  extension "internal";
}

type component CT {
  var integer ct_calls := 0;
  port PT pt;
}

type record Rec {
  integer num,
  charstring str
}

type record of integer IntList;

type union Uni {
  integer i,
  octetstring os,
  universal charstring ucs
}

} // end of module
