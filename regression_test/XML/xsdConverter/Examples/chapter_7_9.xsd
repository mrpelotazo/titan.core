<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.chapter_7_9.org">


	<group name="shipAndBill">
	  	<sequence>
		    <element name="shipTo" type="string"/>
    		<element name="billTo" type="string"/>
  		</sequence>
	</group>


	<group name="shipOrBill">
  		<choice>
    		<element name="shipTo" type="string"/>
    		<element name="billTo" type="string"/>
  		</choice>
	</group>


	<group name="shipAndBillAll">
  		<all>
    		<element name="shipTo" type="string"/>
    		<element name="billTo" type="string"/>
  		</all>
	</group>


</schema>
