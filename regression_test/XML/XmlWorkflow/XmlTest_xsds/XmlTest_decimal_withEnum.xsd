<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:ns="www.XmlTest.org/decimal"
            targetNamespace="www.XmlTest.org/decimal_withEnum"
                  xmlns:dwe="www.XmlTest.org/decimal_withEnum" >
  <xsd:annotation>
    <xsd:documentation xml:lang="en">
      This documentum tests based on
      XML Schema Part 0: Primer Second Edition,
      2.3 Simple Types and
      Appendix B

    </xsd:documentation>
  </xsd:annotation>

<xsd:simpleType name='DecimalEnum' >
  <xsd:restriction base='xsd:decimal'>
    <xsd:enumeration value='2'/>
    <xsd:enumeration value='3.0'/>
    <xsd:enumeration value='4.2'/>
    <xsd:enumeration value='-1000.1'/>
  </xsd:restriction>
</xsd:simpleType>

</xsd:schema>
