module attribs_OK {

type record yuck {
  record of universal charstring aa,
  integer b
}
with {
  variant (aa) "anyAttributes";
  variant (b)  "attribute";
}

type record yuck2 {
  integer b,
  record of universal charstring aa
}
with {
  variant (aa) "anyAttributes";
  variant (b)  "attribute";
}


type record yuck_uo {
  record of enumerated { y } order,
  record of universal charstring aa,
  integer b,
  boolean y
}
with {
  variant "useOrder";
  variant (aa) "anyAttributes";
  variant (b)  "attribute";
}


}
with {
  encode "XML";
}

