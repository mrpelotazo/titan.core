module disorder {	//^In TTCN-3 module `disorder'://

type record marx { //^In type definition//
  record of enumerated { //^error: Enumeration values must start at 0 and have no gaps//
    chico(2), groucho, harpo
  } order,
  integer chico,
  float   groucho,
  boolean harpo
}
with {
  variant "useOrder";
}

}
with {
  encode "XML"
}
