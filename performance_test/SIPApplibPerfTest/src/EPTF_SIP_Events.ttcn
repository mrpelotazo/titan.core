//  c_SIP_eventName_retransmittedRequest - "Retransmitted request"
//  c_SIP_eventName_unknownSubscription - "Unknown subscription"
//  c_SIP_eventName_retransmittedResponse - "Retransmitted response"
//  c_SIP_eventName_NOTIFYActive - "NOTIFY: Subscription active"
//  c_SIP_eventName_NOTIFYPending - "NOTIFY: Subscription pending"
//  c_SIP_eventName_NOTIFYTerminated - "NOTIFY: Subscription terminated"
//  c_SIP_eventName_retryAfter - "Retry after"
//  c_SIP_eventName_UACTransactionTimeout - "Client transaction timeout"
//  c_SIP_eventName_UASTransactionTimeout - "Server transaction timeout"
//  c_SIP_eventName_requestOutOfOrder - "Request in dialog arrived out of order"
//  c_SIP_eventName_userNotFound - "User has not been found in DB"
//  c_SIP_eventName_responseNotFoundForPRACK - "Response has not been found for PRACK"
//  c_SIP_eventName_provisionalRespOutOfOrder - "Provisional response out of order"
//  c_SIP_eventName_unknownPublication - "Unknown publication"
//  c_SIP_eventName_invalidTransactionId - "Invalid transaction Id"
//  c_SIP_eventName_ACKto2xx - "ACK to 2xx"
//  c_SIP_eventName_RespWithoutContext - "Response without context"
//  c_SIP_eventName_unknownResourceForPUBLISH - "PUBLISH received with SIP-If-Match for unknown resource"
//  c_SIP_eventName_unknownPUBLISH - "unknown SIP-If-Match received in PUBLISH"
//  
////////////////////////////////////////////////////
