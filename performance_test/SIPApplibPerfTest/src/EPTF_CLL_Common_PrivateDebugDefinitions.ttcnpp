///////////////////////////////////////////////////////////
//  Constant: c_EPTF_Common_debugSwitchPrivate
// 
//  Purpose:
//    Boolean constant to switch on/off debugging in the CLL code
// 
//  Detailed Comments:
//    The actual value depends on the preprocessor macro EPTF_DEBUG.
//    If EPTF_DEBUG is defined, the value is true.
///////////////////////////////////////////////////////////
#ifdef EPTF_DEBUG
const boolean c_EPTF_Common_debugSwitchPrivate := true;
#else   
const boolean c_EPTF_Common_debugSwitchPrivate := false;
#endif

}
